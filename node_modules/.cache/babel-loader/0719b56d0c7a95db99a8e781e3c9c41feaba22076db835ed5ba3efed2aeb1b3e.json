{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport \"core-js/modules/esnext.iterator.map.js\";\nimport axios from 'axios';\nexport default {\n  name: 'ConcernPage',\n  data() {\n    return {\n      searchQuery: '',\n      showModal: false,\n      selectedConcern: null,\n      concerns: []\n    };\n  },\n  computed: {\n    filteredConcerns() {\n      if (!this.searchQuery) return this.concerns;\n      const query = this.searchQuery.toLowerCase();\n      return this.concerns.filter(concern => concern.title.toLowerCase().includes(query));\n    }\n  },\n  methods: {\n    async fetchConcerns() {\n      try {\n        const response = await axios.get('http://127.0.0.1:8000/api/complaints');\n        this.concerns = response.data.data.map(item => ({\n          id: item.id,\n          title: item.subject,\n          description: item.description,\n          submittedBy: item.email,\n          date: new Date(item.created_at).toLocaleDateString(),\n          address: item.address,\n          image: item.file_path || '',\n          isVerified: item.is_verified,\n          isAssigned: item.is_assigned,\n          assignedTo: item.assigned_to,\n          assignedDate: item.assigned_date\n        }));\n      } catch (error) {\n        console.error('Error fetching concerns:', error);\n      }\n    },\n    openModal(concern) {\n      this.selectedConcern = concern;\n      this.showModal = true;\n    },\n    async verifyConcern() {\n      try {\n        await axios.put(`http://127.0.0.1:8000/api/complaints/${this.selectedConcern.id}`, {\n          is_verified: 1\n        });\n        alert(`Concern \"${this.selectedConcern.title}\" has been verified.`);\n        this.showModal = false;\n        this.fetchConcerns(); // Refresh the list\n      } catch (error) {\n        console.error('Error verifying concern:', error);\n      }\n    },\n    async rejectConcern() {\n      try {\n        await axios.put(`http://127.0.0.1:8000/api/complaintss/${this.selectedConcern.id}`, {\n          is_verified: 9\n        });\n        alert(`Concern \"${this.selectedConcern.title}\" has been rejected.`);\n        this.showModal = false;\n        this.fetchConcerns(); // Refresh the list\n      } catch (error) {\n        console.error('Error rejecting concern:', error);\n      }\n    }\n  },\n  mounted() {\n    this.fetchConcerns();\n  }\n};","map":{"version":3,"names":["axios","name","data","searchQuery","showModal","selectedConcern","concerns","computed","filteredConcerns","query","toLowerCase","filter","concern","title","includes","methods","fetchConcerns","response","get","map","item","id","subject","description","submittedBy","email","date","Date","created_at","toLocaleDateString","address","image","file_path","isVerified","is_verified","isAssigned","is_assigned","assignedTo","assigned_to","assignedDate","assigned_date","error","console","openModal","verifyConcern","put","alert","rejectConcern","mounted"],"sources":["C:\\Users\\darle\\solid-waste-management-system\\src\\components\\ConcernPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"space-y-8\">\r\n    <div class=\"max-w-4xl mx-auto p-6 bg-white rounded-lg shadow-md\">\r\n      <h2 class=\"text-xl font-semibold text-gray-800\">Concerns</h2>\r\n\r\n      <!-- Search Box -->\r\n      <div class=\"mb-4\">\r\n        <input\r\n          type=\"text\"\r\n          v-model=\"searchQuery\"\r\n          placeholder=\"Search concerns...\"\r\n          class=\"w-full p-2 border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-green-500\"\r\n        />\r\n      </div>\r\n\r\n      <!-- List of Concerns -->\r\n      <div class=\"space-y-4\">\r\n        <div\r\n          v-for=\"concern in filteredConcerns\"\r\n          :key=\"concern.id\"\r\n          @click=\"openModal(concern)\"\r\n          class=\"p-4 bg-gray-100 rounded-lg shadow-sm cursor-pointer hover:bg-gray-200\"\r\n        >\r\n          <h3 class=\"text-lg font-medium text-gray-800\">{{ concern.title }}</h3>\r\n          <p class=\"text-sm text-gray-600\">Submitted by: {{ concern.submittedBy }}</p>\r\n          <p class=\"text-sm text-gray-600\">Date: {{ concern.date }}</p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Concern Details Modal -->\r\n    <Dialog\r\n      v-if=\"selectedConcern\"\r\n      header=\"Concern Details\"\r\n      v-model:visible=\"showModal\"\r\n      modal\r\n      class=\"concern-modal\"\r\n      style=\"width: 40vw; max-width: 600px;\"\r\n    >\r\n      <div>\r\n        <h3 class=\"text-lg font-semibold text-gray-800\">{{ selectedConcern.title }}</h3>\r\n        <p class=\"mt-2 text-gray-700\">{{ selectedConcern.description }}</p>\r\n\r\n        <!-- Attached Image -->\r\n        <div v-if=\"selectedConcern.image\" class=\"mt-4\">\r\n          <p class=\"text-sm font-medium text-gray-700\">Attached Image:</p>\r\n          <img :src=\"selectedConcern.image\" alt=\"Concern Image\" class=\"w-full h-auto mt-2 rounded-lg\" />\r\n        </div>\r\n\r\n        <!-- Additional Info -->\r\n        <div class=\"mt-4\">\r\n          <p class=\"text-sm text-gray-600\"><strong>Submitted by:</strong> {{ selectedConcern.submittedBy }}</p>\r\n          <p class=\"text-sm text-gray-600\"><strong>Date:</strong> {{ selectedConcern.date }}</p>\r\n          <p class=\"text-sm text-gray-600\"><strong>Address:</strong> {{ selectedConcern.address }}</p>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"mt-6 flex justify-end space-x-4\">\r\n        <button\r\n          class=\"px-4 py-2 bg-green-600 text-white rounded-lg hover:bg-green-700\"\r\n          @click=\"verifyConcern\"\r\n        >\r\n          Verify\r\n        </button>\r\n        <button\r\n          class=\"px-4 py-2 bg-red-600 text-white rounded-lg hover:bg-red-700\"\r\n          @click=\"rejectConcern\"\r\n        >\r\n          Reject\r\n        </button>\r\n      </div>\r\n    </Dialog>\r\n  </div>\r\n</template>\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'ConcernPage',\r\n  data() {\r\n    return {\r\n      searchQuery: '',\r\n      showModal: false,\r\n      selectedConcern: null,\r\n      concerns: [],\r\n    };\r\n  },\r\n  computed: {\r\n    filteredConcerns() {\r\n      if (!this.searchQuery) return this.concerns;\r\n      const query = this.searchQuery.toLowerCase();\r\n      return this.concerns.filter((concern) =>\r\n        concern.title.toLowerCase().includes(query)\r\n      );\r\n    },\r\n  },\r\n  methods: {\r\n    async fetchConcerns() {\r\n      try {\r\n        const response = await axios.get('http://127.0.0.1:8000/api/complaints');\r\n        this.concerns = response.data.data.map((item) => ({\r\n          id: item.id,\r\n          title: item.subject,\r\n          description: item.description,\r\n          submittedBy: item.email,\r\n          date: new Date(item.created_at).toLocaleDateString(),\r\n          address: item.address,\r\n          image: item.file_path || '',\r\n          isVerified: item.is_verified,\r\n          isAssigned: item.is_assigned,\r\n          assignedTo: item.assigned_to,\r\n          assignedDate: item.assigned_date,\r\n        }));\r\n      } catch (error) {\r\n        console.error('Error fetching concerns:', error);\r\n      }\r\n    },\r\n    openModal(concern) {\r\n      this.selectedConcern = concern;\r\n      this.showModal = true;\r\n    },\r\n    async verifyConcern() {\r\n      try {\r\n        await axios.put(`http://127.0.0.1:8000/api/complaints/${this.selectedConcern.id}`, {\r\n          is_verified: 1,\r\n        });\r\n        alert(`Concern \"${this.selectedConcern.title}\" has been verified.`);\r\n        this.showModal = false;\r\n        this.fetchConcerns(); // Refresh the list\r\n      } catch (error) {\r\n        console.error('Error verifying concern:', error);\r\n      }\r\n    },\r\n    async rejectConcern() {\r\n      try {\r\n        await axios.put(`http://127.0.0.1:8000/api/complaintss/${this.selectedConcern.id}`, {\r\n          is_verified: 9,\r\n        });\r\n        alert(`Concern \"${this.selectedConcern.title}\" has been rejected.`);\r\n        this.showModal = false;\r\n        this.fetchConcerns(); // Refresh the list\r\n      } catch (error) {\r\n        console.error('Error rejecting concern:', error);\r\n      }\r\n    },\r\n  },\r\n  mounted() {\r\n    this.fetchConcerns();\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n<style scoped>\r\n/* Dialog modal customization */\r\n.concern-modal .p-dialog-content {\r\n  font-size: 0.875rem;\r\n}\r\n</style>\r\n"],"mappings":";;;AA2EA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,SAAS,EAAE,KAAK;MAChBC,eAAe,EAAE,IAAI;MACrBC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,gBAAgBA,CAAA,EAAG;MACjB,IAAI,CAAC,IAAI,CAACL,WAAW,EAAE,OAAO,IAAI,CAACG,QAAQ;MAC3C,MAAMG,KAAI,GAAI,IAAI,CAACN,WAAW,CAACO,WAAW,CAAC,CAAC;MAC5C,OAAO,IAAI,CAACJ,QAAQ,CAACK,MAAM,CAAEC,OAAO,IAClCA,OAAO,CAACC,KAAK,CAACH,WAAW,CAAC,CAAC,CAACI,QAAQ,CAACL,KAAK,CAC5C,CAAC;IACH;EACF,CAAC;EACDM,OAAO,EAAE;IACP,MAAMC,aAAaA,CAAA,EAAG;MACpB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMjB,KAAK,CAACkB,GAAG,CAAC,sCAAsC,CAAC;QACxE,IAAI,CAACZ,QAAO,GAAIW,QAAQ,CAACf,IAAI,CAACA,IAAI,CAACiB,GAAG,CAAEC,IAAI,KAAM;UAChDC,EAAE,EAAED,IAAI,CAACC,EAAE;UACXR,KAAK,EAAEO,IAAI,CAACE,OAAO;UACnBC,WAAW,EAAEH,IAAI,CAACG,WAAW;UAC7BC,WAAW,EAAEJ,IAAI,CAACK,KAAK;UACvBC,IAAI,EAAE,IAAIC,IAAI,CAACP,IAAI,CAACQ,UAAU,CAAC,CAACC,kBAAkB,CAAC,CAAC;UACpDC,OAAO,EAAEV,IAAI,CAACU,OAAO;UACrBC,KAAK,EAAEX,IAAI,CAACY,SAAQ,IAAK,EAAE;UAC3BC,UAAU,EAAEb,IAAI,CAACc,WAAW;UAC5BC,UAAU,EAAEf,IAAI,CAACgB,WAAW;UAC5BC,UAAU,EAAEjB,IAAI,CAACkB,WAAW;UAC5BC,YAAY,EAAEnB,IAAI,CAACoB;QACrB,CAAC,CAAC,CAAC;MACL,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,0BAA0B,EAAEA,KAAK,CAAC;MAClD;IACF,CAAC;IACDE,SAASA,CAAC/B,OAAO,EAAE;MACjB,IAAI,CAACP,eAAc,GAAIO,OAAO;MAC9B,IAAI,CAACR,SAAQ,GAAI,IAAI;IACvB,CAAC;IACD,MAAMwC,aAAaA,CAAA,EAA