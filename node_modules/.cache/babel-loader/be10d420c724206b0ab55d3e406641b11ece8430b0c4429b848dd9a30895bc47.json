 // Password input field\r\n      menuItems: [\r\n        { label: 'Announcement', icon: 'pi pi-megaphone', command: () => this.navigateTo('announcement') },\r\n        { label: 'Concerns', icon: 'pi pi-comments', command: () => this.navigateTo('concerns') },\r\n        { label: 'Messages', icon: 'pi pi-envelope', command: () => this.navigateTo('messages') },\r\n        { label: 'Reports', icon: 'pi pi-file', command: () => this.navigateTo('reports') },\r\n        { label: 'Analytics', icon: 'pi pi-chart-bar', command: () => this.navigateTo('analytics') },\r\n        { label: 'Account Management', icon: 'pi pi-users', command: () => this.navigateTo('account-management') },\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n    login() {\r\n      // Handle login logic here\r\n      console.log('Logging in with:', this.username, this.password);\r\n      this.showLoginModal = false; // Close the modal after login\r\n    },\r\n    navigateTo(route) {\r\n      console.log(`Navigating to ${route}`);\r\n      // Replace with actual navigation logic, e.g., this.$router.push(`/${route}`);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n"],"mappings":"AAgEA,eAAe;EACbA,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,KAAK;MAAE;MACvBC,QAAQ,EAAE,EAAE;MAAE;MACdC,QAAQ,EAAE,EAAE;MAAE;MACdC,SAAS,EAAE,CACT;QAAEC,KAAK,EAAE,cAAc;QAAEC,IAAI,EAAE,iBAAiB;QAAEC,OAAO,EAAEA,CAAA,KAAM,IAAI,CAACC,UAAU,CAAC,cAAc;MAAE,CAAC,EAClG;QAAEH,KAAK,EAAE,UAAU;QAAEC,IAAI,EAAE,gBAAgB;QAAEC,OAAO,EAAEA,CAAA,KAAM,IAAI,CAACC,UAAU,CAAC,UAAU;MAAE,CAAC,EACzF;QAAEH,KAAK,EAAE,UAAU;QAAEC,IAAI,EAAE,gBAAgB;QAAEC,OAAO,EAAEA,CAAA,KAAM,IAAI,CAACC,UAAU,CAAC,UAAU;MAAE,CAAC,EACzF;QAAEH,KAAK,EAAE,SAAS;QAAEC,IAAI,EAAE,YAAY;QAAEC,OAAO,EAAEA,CAAA,KAAM,IAAI,CAACC,UAAU,CAAC,SAAS;MAAE,CAAC,EACnF;QAAEH,KAAK,EAAE,WAAW;QAAEC,IAAI,EAAE,iBAAiB;QAAEC,OAAO,EAAEA,CAAA,KAAM,IAAI,CAACC,UAAU,CAAC,WAAW;MAAE,CAAC,EAC5F;QAAEH,KAAK,EAAE,oBAAoB;QAAEC,IAAI,EAAE,aAAa;QAAEC,OAAO,EAAEA,CAAA,KAAM,IAAI,CAACC,UAAU,CAAC,oBAAoB;MAAE,CAAC;IAE9G,CAAC;