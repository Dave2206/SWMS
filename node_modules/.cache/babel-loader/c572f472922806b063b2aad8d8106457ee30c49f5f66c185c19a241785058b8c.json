{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      tab: 'compose',\n      compose: {\n        recipient: '',\n        subject: '',\n        message: '',\n        attachment: null\n      },\n      inbox: [],\n      sent: []\n    };\n  },\n  mounted() {\n    this.fetchInbox();\n    this.fetchSent();\n  },\n  methods: {\n    async sendMessage() {\n      if (!this.compose.recipient || !this.compose.subject || !this.compose.message) {\n        alert('All fields are required.');\n        return;\n      }\n      const formData = new FormData();\n      formData.append('recipient', this.compose.recipient);\n      formData.append('subject', this.compose.subject);\n      formData.append('message', this.compose.message);\n      if (this.compose.attachment) {\n        formData.append('attachment', this.compose.attachment);\n      }\n      try {\n        await axios.post('http://127.0.0.1:8000/api/messages/send', formData, {\n          headers: {\n            Authorization: `Bearer ${localStorage.getItem('auth_token')}`,\n            'Content-Type': 'multipart/form-data'\n          }\n        });\n        alert('Message sent successfully.');\n        this.fetchSent();\n        this.compose = {\n          recipient: '',\n          subject: '',\n          message: '',\n          attachment: null\n        };\n      } catch (error) {\n        console.error(error);\n        alert('Failed to send message.');\n      }\n    },\n    async fetchInbox() {\n      try {\n        const response = await axios.get('http://127.0.0.1:8000/api/messages/inbox', {\n          headers: {\n            Authorization: `Bearer ${localStorage.getItem('auth_token')}`\n          }\n        });\n        this.inbox = response.data;\n      } catch (error) {\n        console.error(error);\n        alert('Failed to load inbox messages.');\n      }\n    },\n    async fetchSent() {\n      try {\n        const response = await axios.get('http://127.0.0.1:8000/api/messages/sent', {\n          headers: {\n            Authorization: `Bearer ${localStorage.getItem('auth_token')}`\n          }\n        });\n        this.sent = response.data;\n      } catch (error) {\n        console.error(error);\n        alert('Failed to load sent messages.');\n      }\n    },\n    onFileChange(event) {\n      const file = event.target.files[0];\n      if (file) {\n        this.compose.attachment = file;\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","tab","compose","recipient","subject","message","attachment","inbox","sent","mounted","fetchInbox","fetchSent","methods","sendMessage","alert","formData","FormData","append","post","headers","Authorization","localStorage","getItem","error","console","response","get","onFileChange","event","file","target","files"],"sources":["C:\\Users\\darle\\solid-waste-management-system\\src\\components\\MessagePage.vue"],"sourcesContent":["<template>\r\n  <div class=\"space-y-8\">\r\n    <div class=\"max-w-5xl mx-auto p-6 bg-white rounded-lg shadow-md\">\r\n      <h2 class=\"text-xl font-semibold text-gray-800 mb-6\">Messaging</h2>\r\n\r\n      <!-- Tabs -->\r\n      <div class=\"flex border-b border-gray-200 mb-6\">\r\n        <button\r\n          @click=\"tab = 'compose'\"\r\n          class=\"px-4 py-2 focus:outline-none\"\r\n          :class=\"tab === 'compose' ? 'text-green-600 border-b-2 border-green-600' : 'text-gray-600'\"\r\n        >\r\n          Compose\r\n        </button>\r\n        <button\r\n          @click=\"tab = 'inbox'\"\r\n          class=\"px-4 py-2 focus:outline-none\"\r\n          :class=\"tab === 'inbox' ? 'text-green-600 border-b-2 border-green-600' : 'text-gray-600'\"\r\n        >\r\n          Inbox\r\n        </button>\r\n        <button\r\n          @click=\"tab = 'sent'\"\r\n          class=\"px-4 py-2 focus:outline-none\"\r\n          :class=\"tab === 'sent' ? 'text-green-600 border-b-2 border-green-600' : 'text-gray-600'\"\r\n        >\r\n          Sent\r\n        </button>\r\n      </div>\r\n\r\n      <!-- Compose Tab -->\r\n      <div v-if=\"tab === 'compose'\" class=\"space-y-6\">\r\n        <h3 class=\"text-lg font-semibold text-gray-800\">Compose Message</h3>\r\n\r\n        <form @submit.prevent=\"sendMessage\" class=\"space-y-4\">\r\n          <!-- Recipient -->\r\n          <div>\r\n            <label for=\"recipient\" class=\"block text-sm font-medium text-gray-700 mb-1\">Recipient</label>\r\n            <input\r\n              id=\"recipient\"\r\n              v-model=\"compose.recipient\"\r\n              type=\"text\"\r\n              placeholder=\"Enter recipient\"\r\n              class=\"w-full p-3 border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-green-500\"\r\n            />\r\n          </div>\r\n\r\n          <!-- Subject -->\r\n          <div>\r\n            <label for=\"subject\" class=\"block text-sm font-medium text-gray-700 mb-1\">Subject</label>\r\n            <input\r\n              id=\"subject\"\r\n              v-model=\"compose.subject\"\r\n              type=\"text\"\r\n              placeholder=\"Enter subject\"\r\n              class=\"w-full p-3 border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-green-500\"\r\n            />\r\n          </div>\r\n\r\n          <!-- Message -->\r\n          <div>\r\n            <label for=\"message\" class=\"block text-sm font-medium text-gray-700 mb-1\">Message</label>\r\n            <textarea\r\n              id=\"message\"\r\n              v-model=\"compose.message\"\r\n              rows=\"5\"\r\n              placeholder=\"Write your message here...\"\r\n              class=\"w-full p-3 border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-green-500\"\r\n            ></textarea>\r\n          </div>\r\n\r\n          <!-- Attach Image -->\r\n          <div>\r\n            <label for=\"attachment\" class=\"block text-sm font-medium text-gray-700 mb-1\">Attach Image</label>\r\n            <input\r\n              type=\"file\"\r\n              id=\"attachment\"\r\n              accept=\"image/*\"\r\n              @change=\"onFileChange\"\r\n              class=\"w-full p-2 border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-green-500\"\r\n            />\r\n          </div>\r\n\r\n          <!-- Submit Button -->\r\n          <div class=\"flex justify-center\">\r\n            <button\r\n              type=\"submit\"\r\n              class=\"px-6 py-3 bg-green-600 text-white rounded-lg hover:bg-green-700 focus:outline-none focus:ring-2 focus:ring-green-500\"\r\n            >\r\n              Send\r\n            </button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n\r\n      <!-- Inbox Tab -->\r\n      <div v-if=\"tab === 'inbox'\" class=\"space-y-6\">\r\n        <h3 class=\"text-lg font-semibold text-gray-800\">Inbox</h3>\r\n        <ul class=\"bg-white shadow rounded-lg p-4\">\r\n          <li v-for=\"message in inbox\" :key=\"message.id\" class=\"border-b py-2\">\r\n            <strong>{{ message.subject }}</strong> - {{ message.message }}\r\n          </li>\r\n        </ul>\r\n      </div>\r\n\r\n      <!-- Sent Tab -->\r\n      <div v-if=\"tab === 'sent'\" class=\"space-y-6\">\r\n        <h3 class=\"text-lg font-semibold text-gray-800\">Sent Messages</h3>\r\n        <ul class=\"bg-white shadow rounded-lg p-4\">\r\n          <li v-for=\"message in sent\" :key=\"message.id\" class=\"border-b py-2\">\r\n            <strong>{{ message.subject }}</strong> - {{ message.message }}\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      tab: 'compose',\r\n      compose: {\r\n        recipient: '',\r\n        subject: '',\r\n        message: '',\r\n        attachment: null,\r\n      },\r\n      inbox: [],\r\n      sent: [],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchInbox();\r\n    this.fetchSent();\r\n  },\r\n  methods: {\r\n    async sendMessage() {\r\n      if (!this.compose.recipient || !this.compose.subject || !this.compose.message) {\r\n        alert('All fields are required.');\r\n        return;\r\n      }\r\n\r\n      const formData = new FormData();\r\n      formData.append('recipient', this.compose.recipient);\r\n      formData.append('subject', this.compose.subject);\r\n      formData.append('message', this.compose.message);\r\n      if (this.compose.attachment) {\r\n        formData.append('attachment', this.compose.attachment);\r\n      }\r\n\r\n      try {\r\n        await axios.post('http://127.0.0.1:8000/api/messages/send', formData, {\r\n          headers: {\r\n            Authorization: `Bearer ${localStorage.getItem('auth_token')}`,\r\n            'Content-Type': 'multipart/form-data',\r\n          },\r\n        });\r\n        alert('Message sent successfully.');\r\n        this.fetchSent();\r\n        this.compose = { recipient: '', subject: '', message: '', attachment: null };\r\n      } catch (error) {\r\n        console.error(error);\r\n        alert('Failed to send message.');\r\n      }\r\n    },\r\n    async fetchInbox() {\r\n      try {\r\n        const response = await axios.get('http://127.0.0.1:8000/api/messages/inbox', {\r\n          headers: {\r\n            Authorization: `Bearer ${localStorage.getItem('auth_token')}`,\r\n          },\r\n        });\r\n        this.inbox = response.data;\r\n      } catch (error) {\r\n        console.error(error);\r\n        alert('Failed to load inbox messages.');\r\n      }\r\n    },\r\n    async fetchSent() {\r\n      try {\r\n        const response = await axios.get('http://127.0.0.1:8000/api/messages/sent', {\r\n          headers: {\r\n            Authorization: `Bearer ${localStorage.getItem('auth_token')}`,\r\n          },\r\n        });\r\n        this.sent = response.data;\r\n      } catch (error) {\r\n        console.error(error);\r\n        alert('Failed to load sent messages.');\r\n      }\r\n    },\r\n    onFileChange(event) {\r\n      const file = event.target.files[0];\r\n      if (file) {\r\n        this.compose.attachment = file;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nul li:last-child {\r\n  border-bottom: none;\r\n}\r\n</style>\r\n"],"mappings":"AAuHA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,GAAG,EAAE,SAAS;MACdC,OAAO,EAAE;QACPC,SAAS,EAAE,EAAE;QACbC,OAAO,EAAE,EAAE;QACXC,OAAO,EAAE,EAAE;QACXC,UAAU,EAAE;MACd,CAAC;MACDC,KAAK,EAAE,EAAE;MACTC,IAAI,EAAE;IACR,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,UAAU,CAAC,CAAC;IACjB,IAAI,CAACC,SAAS,CAAC,CAAC;EAClB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,WAAWA,CAAA,EAAG;MAClB,IAAI,CAAC,IAAI,CAACX,OAAO,CAACC,SAAQ,IAAK,CAAC,IAAI,CAACD,OAAO,CAACE,OAAM,IAAK,CAAC,IAAI,CAACF,OAAO,CAACG,OAAO,EAAE;QAC7ES,KAAK,CAAC,0BAA0B,CAAC;QACjC;MACF;MAEA,MAAMC,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;MAC/BD,QAAQ,CAACE,MAAM,CAAC,WAAW,EAAE,IAAI,CAACf,OAAO,CAACC,SAAS,CAAC;MACpDY,QAAQ,CAACE,MAAM,CAAC,SAAS,EAAE,IAAI,CAACf,OAAO,CAACE,OAAO,CAAC;MAChDW,QAAQ,CAACE,MAAM,CAAC,SAAS,EAAE,IAAI,CAACf,OAAO,CAACG,OAAO,CAAC;MAChD,IAAI,IAAI,CAACH,OAAO,CAACI,UAAU,EAAE;QAC3BS,QAAQ,CAACE,MAAM,CAAC,YAAY,EAAE,IAAI,CAACf,OAAO,CAACI,UAAU,CAAC;MACxD;MAEA,IAAI;QACF,MAAMP,KAAK,CAACmB,IAAI,CAAC,yCAAyC,EAAEH,QAAQ,EAAE;UACpEI,OAAO,EAAE;YACPC,aAAa,EAAE,UAAUC,YAAY,CAACC,OAAO,CAAC,YAAY,CAAC,EAAE;YAC7D,cAAc,EAAE;UAClB;QACF,CAAC,CAAC;QACFR,KAAK,CAAC,4BAA4B,CAAC;QACnC,IAAI,CAACH,SAAS,CAAC,CAAC;QAChB,IAAI,CAACT,OAAM,GAAI;UAAEC,SAAS,EAAE,EAAE;UAAEC,OAAO,EAAE,EAAE;UAAEC,OAAO,EAAE,EAAE;UAAEC,UAAU,EAAE;QAAK,CAAC;MAC9E,EAAE,OAAOiB,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;QACpBT,KAAK,CAAC,yBAAyB,CAAC;MAClC;IACF,CAAC;IACD,MAAMJ,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMe,QAAO,GAAI,MAAM1B,KAAK,CAAC2B,GAAG,CAAC,0CAA0C,EAAE;UAC3EP,OAAO,EAAE;YACPC,aAAa,EAAE,UAAUC,YAAY,CAACC,OAAO,CAAC,YAAY,CAAC;UAC7D;QACF,CAAC,CAAC;QACF,IAAI,CAACf,KAAI,GAAIkB,QAAQ,CAACzB,IAAI;MAC5B,EAAE,OAAOuB,KAAK,EAAE;QA