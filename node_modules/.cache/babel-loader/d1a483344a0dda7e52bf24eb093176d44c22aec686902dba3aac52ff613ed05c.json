{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport \"core-js/modules/esnext.iterator.map.js\";\nimport axios from 'axios';\nexport default {\n  name: 'ConcernPage',\n  data() {\n    return {\n      searchQuery: '',\n      showModal: false,\n      selectedConcern: null,\n      concerns: [],\n      activeTab: 'pending' // default to 'pending' tab\n    };\n  },\n  computed: {\n    filteredConcerns() {\n      const filtered = this.concerns.filter(concern => {\n        if (this.activeTab === 'pending') return concern.isVerified === 0;\n        if (this.activeTab === 'assigned') return concern.isAssigned === null;\n        return true; // Show all concerns for the 'all' tab\n      });\n      return this.searchQuery ? filtered.filter(concern => concern.title.toLowerCase().includes(this.searchQuery.toLowerCase())) : filtered;\n    }\n  },\n  methods: {\n    // Fetch concerns based on active tab\n    async fetchConcerns() {\n      let url = 'http://127.0.0.1:8000/api/complaints';\n      if (this.activeTab === 'pending') {\n        url = 'http://127.0.0.1:8000/api/complaints/verification';\n      } else if (this.activeTab === 'assigned') {\n        url = 'http://127.0.0.1:8000/api/complaints/assignment';\n      }\n      try {\n        const response = await axios.get(url);\n        this.concerns = response.data.data.map(item => ({\n          id: item.id,\n          title: item.subject,\n          description: item.description,\n          submittedBy: item.email,\n          date: new Date(item.created_at).toLocaleDateString(),\n          address: item.address,\n          image: item.file_path || '',\n          isVerified: item.is_verified,\n          isAssigned: item.is_assigned,\n          assignedTo: item.assigned_to,\n          assignedDate: item.assigned_date\n        }));\n      } catch (error) {\n        console.error('Error fetching concerns:', error);\n      }\n    },\n    openModal(concern) {\n      this.selectedConcern = concern;\n      this.showModal = true;\n    },\n    async verifyConcern() {\n      try {\n        await axios.put(`http://127.0.0.1:8000/api/complaints/${this.selectedConcern.id}`, {\n          is_verified: 1\n        });\n        alert(`Concern \"${this.selectedConcern.title}\" has been verified.`);\n        this.fetchConcerns();\n      } catch (error) {\n        console.error('Error verifying concern:', error);\n      }\n    },\n    async rejectConcern() {\n      try {\n        await axios.put(`http://127.0.0.1:8000/api/complaints/${this.selectedConcern.id}`, {\n          is_verified: 9\n        });\n        alert(`Concern \"${this.selectedConcern.title}\" has been rejected.`);\n        this.fetchConcerns();\n      } catch (error) {\n        console.error('Error rejecting concern:', error);\n      }\n    },\n    async assignConcern() {\n      try {\n        await axios.put(`http://127.0.0.1:8000/api/complaints/${this.selectedConcern.id}`, {\n          is_assigned: 1\n        });\n        alert(`Concern \"${this.selectedConcern.title}\" has been assigned.`);\n        this.fetchConcerns();\n      } catch (error) {\n        console.error('Error assigning concern:', error);\n      }\n    }\n  },\n  watch: {\n    activeTab() {\n      this.fetchConcerns(); // Re-fetch concerns when tab changes\n    }\n  },\n  mounted() {\n    this.fetchConcerns();\n  }\n};","map":{"version":3,"names":["axios","name","data","searchQuery","showModal","selectedConcern","concerns","activeTab","computed","filteredConcerns","filtered","filter","concern","isVerified","isAssigned","title","toLowerCase","includes","methods","fetchConcerns","url","response","get","map","item","id","subject","description","submittedBy","email","date","Date","created_at","toLocaleDateString","address","image","file_path","is_verified","is_assigned","assignedTo","assigned_to","assignedDate","assigned_date","error","console","openModal","verifyConcern","put","alert","rejectConcern","assignConcern","watch","mounted"],"sources":["C:\\Users\\darle\\solid-waste-management-system\\src\\components\\ConcernPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"space-y-8\">\r\n    <div class=\"max-w-4xl mx-auto p-6 bg-white rounded-lg shadow-md\">\r\n      <h2 class=\"text-xl font-semibold text-gray-800\">Concerns Management</h2>\r\n\r\n      <!-- Tabs for Different Stages -->\r\n      <div class=\"flex space-x-4 mb-4\">\r\n        <button @click=\"activeTab = 'pending'\" :class=\"{ 'bg-green-500 text-white': activeTab === 'pending' }\" class=\"px-4 py-2 rounded-lg\">Pending Verification</button>\r\n        <button @click=\"activeTab = 'assigned'\" :class=\"{ 'bg-blue-500 text-white': activeTab === 'assigned' }\" class=\"px-4 py-2 rounded-lg\">Assigned Concerns</button>\r\n        <button @click=\"activeTab = 'all'\" :class=\"{ 'bg-gray-500 text-white': activeTab === 'all' }\" class=\"px-4 py-2 rounded-lg\">All Concerns</button>\r\n      </div>\r\n\r\n      <!-- Search Box -->\r\n      <div class=\"mb-4\">\r\n        <input type=\"text\" v-model=\"searchQuery\" placeholder=\"Search concerns...\" class=\"w-full p-2 border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-green-500\" />\r\n      </div>\r\n\r\n      <!-- List of Concerns -->\r\n      <div class=\"space-y-4\">\r\n        <div v-for=\"concern in filteredConcerns\" :key=\"concern.id\" @click=\"openModal(concern)\" class=\"p-4 bg-gray-100 rounded-lg shadow-sm cursor-pointer hover:bg-gray-200\">\r\n          <h3 class=\"text-lg font-medium text-gray-800\">{{ concern.title }}</h3>\r\n          <p class=\"text-sm text-gray-600\">Submitted by: {{ concern.submittedBy }}</p>\r\n          <p class=\"text-sm text-gray-600\">Date: {{ concern.date }}</p>\r\n          <p v-if=\"concern.isVerified === 1\" class=\"text-sm text-green-600\">Verified</p>\r\n          <p v-if=\"concern.isVerified === 9\" class=\"text-sm text-red-600\">Rejected</p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Concern Details Modal -->\r\n    <Dialog v-if=\"selectedConcern\" header=\"Concern Details\" v-model:visible=\"showModal\" modal class=\"concern-modal\" style=\"width: 40vw; max-width: 600px;\">\r\n      <div>\r\n        <h3 class=\"text-lg font-semibold text-gray-800\">{{ selectedConcern.title }}</h3>\r\n        <p class=\"mt-2 text-gray-700\">{{ selectedConcern.description }}</p>\r\n\r\n        <!-- Attached Image -->\r\n        <div v-if=\"selectedConcern.image\" class=\"mt-4\">\r\n          <p class=\"text-sm font-medium text-gray-700\">Attached Image:</p>\r\n          <img :src=\"selectedConcern.image\" alt=\"Concern Image\" class=\"w-full h-auto mt-2 rounded-lg\" />\r\n        </div>\r\n\r\n        <!-- Additional Info -->\r\n        <div class=\"mt-4\">\r\n          <p class=\"text-sm text-gray-600\"><strong>Submitted by:</strong> {{ selectedConcern.submittedBy }}</p>\r\n          <p class=\"text-sm text-gray-600\"><strong>Date:</strong> {{ selectedConcern.date }}</p>\r\n          <p class=\"text-sm text-gray-600\"><strong>Address:</strong> {{ selectedConcern.address }}</p>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"mt-6 flex justify-end space-x-4\">\r\n        <button v-if=\"selectedConcern.isVerified === 0\" class=\"px-4 py-2 bg-green-600 text-white rounded-lg hover:bg-green-700\" @click=\"verifyConcern\">Verify</button>\r\n        <button v-if=\"selectedConcern.isVerified === 0\" class=\"px-4 py-2 bg-red-600 text-white rounded-lg hover:bg-red-700\" @click=\"rejectConcern\">Reject</button>\r\n        <button v-if=\"selectedConcern.isVerified === 1 && !selectedConcern.isAssigned\" class=\"px-4 py-2 bg-blue-600 text-white rounded-lg hover:bg-blue-700\" @click=\"assignConcern\">Assign</button>\r\n      </div>\r\n    </Dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'ConcernPage',\r\n  data() {\r\n    return {\r\n      searchQuery: '',\r\n      showModal: false,\r\n      selectedConcern: null,\r\n      concerns: [],\r\n      activeTab: 'pending', // default to 'pending' tab\r\n    };\r\n  },\r\n  computed: {\r\n    filteredConcerns() {\r\n      const filtered = this.concerns.filter(concern => {\r\n        if (this.activeTab === 'pending') return concern.isVerified === 0;\r\n        if (this.activeTab === 'assigned') return concern.isAssigned === null;\r\n        return true; // Show all concerns for the 'all' tab\r\n      });\r\n      return this.searchQuery ? filtered.filter(concern => concern.title.toLowerCase().includes(this.searchQuery.toLowerCase())) : filtered;\r\n    },\r\n  },\r\n  methods: {\r\n    // Fetch concerns based on active tab\r\n    async fetchConcerns() {\r\n      let url = 'http://127.0.0.1:8000/api/complaints';\r\n      \r\n      if (this.activeTab === 'pending') {\r\n        url = 'http://127.0.0.1:8000/api/complaints/verification';\r\n      } else if (this.activeTab === 'assigned') {\r\n        url = 'http://127.0.0.1:8000/api/complaints/assignment';\r\n      }\r\n\r\n      try {\r\n        const response = await axios.get(url);\r\n        this.concerns = response.data.data.map((item) => ({\r\n          id: item.id,\r\n          title: item.subject,\r\n          description: item.description,\r\n          submittedBy: item.email,\r\n          date: new Date(item.created_at).toLocaleDateString(),\r\n          address: item.address,\r\n          image: item.file_path || '',\r\n          isVerified: item.is_verified,\r\n          isAssigned: item.is_assigned,\r\n          assignedTo: item.assigned_to,\r\n          assignedDate: item.assigned_date,\r\n        }));\r\n      } catch (error) {\r\n        console.error('Error fetching concerns:', error);\r\n      }\r\n    },\r\n    openModal(concern) {\r\n      this.selectedConcern = concern;\r\n      this.showModal = true;\r\n    },\r\n    async verifyConcern() {\r\n      try {\r\n        await axios.put(`http://127.0.0.1:8000/api/complaints/${this.selectedConcern.id}`, { is_verified: 1 });\r\n        alert(`Concern \"${this.selectedConcern.title}\" has been verified.`);\r\n        this.fetchConcerns();\r\n      } catch (error) {\r\n        console.error('Error verifying concern:', error);\r\n      }\r\n    },\r\n    async rejectConcern() {\r\n      try {\r\n        await axios.put(`http://127.0.0.1:8000/api/complaints/${this.selectedConcern.id}`, { is_verified: 9 });\r\n        alert(`Concern \"${this.selectedConcern.title}\" has been rejected.`);\r\n        this.fetchConcerns();\r\n      } catch (error) {\r\n        console.error('Error rejecting concern:', error);\r\n      }\r\n    },\r\n    async assignConcern() {\r\n      try {\r\n        await axios.put(`http://127.0.0.1:8000/api/complaints/${this.selectedConcern.id}`, { is_assigned: 1 });\r\n        alert(`Concern \"${this.selectedConcern.title}\" has been assigned.`);\r\n        this.fetchConcerns();\r\n      } catch (error) {\r\n        console.error('Error assigning concern:', error);\r\n      }\r\n    },\r\n  },\r\n  watch: {\r\n    activeTab() {\r\n      this.fetchConcerns(); // Re-fetch concerns when tab changes\r\n    },\r\n  },\r\n  mounted() {\r\n    this.fetchConcerns();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Dialog modal customization */\r\n.concern-modal .p-dialog-content {\r\n  font-size: 0.875rem;\r\n}\r\n</style>\r\n"],"mappings":";;;AA2DA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,SAAS,EAAE,KAAK;MAChBC,eAAe,EAAE,IAAI;MACrBC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE,SAAS,CAAE;IACxB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,gBAAgBA,CAAA,EAAG;MACjB,MAAMC,QAAO,GAAI,IAAI,CAACJ,QAAQ,CAACK,MAAM,CAACC,OAAM,IAAK;QAC/C,IAAI,IAAI,CAACL,SAAQ,KAAM,SAAS,EAAE,OAAOK,OAAO,CAACC,UAAS,KAAM,CAAC;QACjE,IAAI,IAAI,CAACN,SAAQ,KAAM,UAAU,EAAE,OAAOK,OAAO,CAACE,UAAS,KAAM,IAAI;QACrE,OAAO,IAAI,EAAE;MACf,CAAC,CAAC;MACF,OAAO,IAAI,CAACX,WAAU,GAAIO,QAAQ,CAACC,MAAM,CAACC,OAAM,IAAKA,OAAO,CA